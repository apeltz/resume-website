{
  "name": "resume-template",
  "version": "1.0.0",
  "description": "boilerplate resume & portfolio template",
  "main": "index.html",
  "scripts": {
    "dev": "yarn -s && webpack-dev-server --progress --colors",
    "build": "yarn -s && webpack --progress --colors",
    "lint": "yarn -s && yarn eslint ./app",
    "deploy-s3": "yarn -s && yarn build && aws s3 sync dist/ s3://www.aaronpeltz.com"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.24.1",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.19.0",
    "eslint-config-synacor": "^1.0.0",
    "eslint-plugin-react": "^7.0.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "react-hot-loader": "^4.0.0-beta.17",
    "style-loader": "^0.13.1",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "aws-sdk": "^2.391.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  }
}
